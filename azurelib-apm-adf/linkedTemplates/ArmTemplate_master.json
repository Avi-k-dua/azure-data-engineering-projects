{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azurelib-apm-adf"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-gm-azurelib.vault.azure.net/"
		},
		"tr_storage_properties_pl_run_validation_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"tr_storage_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a99520ae-ae45-4be4-9629-98fe3f487fc0/resourceGroups/azurelib-rg/providers/Microsoft.Storage/storageAccounts/azurelibdl"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azurelib-apm-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"tr_storage_properties_pl_run_validation_parameters_fileName": {
						"value": "[parameters('tr_storage_properties_pl_run_validation_parameters_fileName')]"
					},
					"tr_storage_properties_typeProperties_scope": {
						"value": "[parameters('tr_storage_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}