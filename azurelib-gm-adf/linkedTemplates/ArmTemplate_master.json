{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azurelib-gm-adf"
		},
		"ls_adls_azurelibdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_azurelibdl'"
		},
		"ls_sql_azurelib_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_azurelib'"
		},
		"ls_adls_azurelibdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurelibdl.dfs.core.windows.net/"
		},
		"ls_amazonS3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_keyvault",
					"type": "LinkedServiceReference"
				},
				"secretName": "s3accessid"
			}
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-gm-azurelib.vault.azure.net/"
		},
		"tr_adls_staging_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a99520ae-ae45-4be4-9629-98fe3f487fc0/resourceGroups/azurelib-rg/providers/Microsoft.Storage/storageAccounts/azurelibdl"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azurelib-gm-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_azurelibdl_accountKey": {
						"value": "[parameters('ls_adls_azurelibdl_accountKey')]"
					},
					"ls_sql_azurelib_connectionString": {
						"value": "[parameters('ls_sql_azurelib_connectionString')]"
					},
					"ls_adls_azurelibdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_azurelibdl_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_amazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_amazonS3_properties_typeProperties_accessKeyId')]"
					},
					"tr_adls_staging_properties_typeProperties_scope": {
						"value": "[parameters('tr_adls_staging_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}